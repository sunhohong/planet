<%= show_for @category do |s| %>
  <%= s.attribute :name %>
  <%= s.attribute :parent_id %>
  <%= s.attribute :lft %>
  <%= s.attribute :rgt %>
  <%= s.attribute :seq %>
<% end %>

<div class="form-actions">
  <%= link_to 'Edit', edit_category_path(@category), :class=>"btn" %>
  <%= link_to 'Back', categories_path, :class=>"btn" %>
</div>
